import math
class Rectangle():
    def __init__(self, a, b):
        self.a = a
        self.b = b
    def field(self):
        if self.a <= 0 or self.b <= 0:
            print("The sides have to be greater than 0")
        else:
            f = self.a * self.b
            print("Field is equal " + str(f))
    def circum(self):
        if self.a <= 0 or self.b <= 0:
            print("The sides have to be greater than 0")
        else:
            l = 2 * self.a + 2 * self.b
            print("Circumference is equal " + str(l))
    def diagonal(self):
        if self.a <= 0 or self.b <= 0:
            print("The sides have to be greater than 0")
        else:
            d = math.sqrt(self.a^2 + self.b^2)
            print("The diagonal is equal " + str(d))
    def tople(self):
        if self.a <= 0 or self.b <= 0:
            print("The sides have to be greater than 0")
        else:
            f = self.a * self.b
            print("Field is equal " + str(f))
            l = 2 * self.a + 2 * self.b
            print("Circumference is equal " + str(l))
            d = math.sqrt(self.a ^ 2 + self.b ^ 2)
            print("The diagonal is equal " + str(d))
